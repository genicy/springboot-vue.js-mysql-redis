<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootdemo.mapper.UserMapper">
    <select id="select" parameterType="java.lang.String" resultType="com.example.springbootdemo.bean.User">
        select * from user where username= #{username}
    </select>
    <select id="insertUser" parameterType="com.example.springbootdemo.bean.User">
        insert into user(username,password) values (#{username},#{password})
    </select>
</mapper>